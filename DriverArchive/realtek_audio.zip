        //
        function changeLang(lang) {
            var currentUrl = window.location.href;

            var url = new URL(currentUrl);
            var params = new URLSearchParams(url.search);
            params.delete("lang");

            // menu_idmenu_id
            var menu_id = getParameterByName("menu_id", window.location.href);
            var link_type = "normal";
            var zoo_list_link = "";

            if(menu_id != null && menu_id != undefined) {
                menu_id = menu_id.replace(/\D/g, "");
            }

            $.ajax({
                type: "POST",
                async: false,
                cache: false,
                data: {
                    menu_id: menu_id,
                    target_lang: lang,
                    url: window.location.href
                },
                dataType: "json",
                url: '/Home/GetAssociationMenuId',
                success: function (result) {
                    if (result.Data) {
                        link_type = result.Data.link_type;
                        zoo_list_link = result.Data.zoo_list_link;
                        if (menu_id)
                            menu_id = result.Data.asso_menu_id;
                    }

                },
                error: function (result) {
                    showError(result);
                },
            });

            params.delete("menu_id");
            params.append("menu_id", menu_id);



            if (link_type == "zoo") {

                window.location.href = zoo_list_link + "&lang=" + lang;

            } else {
                if (params.size == 0)
                    window.location.href = url.pathname + "?lang=" + lang;
                else if (menu_id == "-1" || menu_id == null)
                    // menu
                    window.location.href = "https://" + window.location.host + "?lang=" + lang;
                else
                    window.location.href = url.pathname + "?lang=" + lang + "&" + params.toString();
            }
        }

