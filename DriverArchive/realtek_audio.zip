<script>
    var menu_od;

    // 
    var footer_menu_order = {
        "tw": ["", "", "&", "", "", "", "", "",
        ],
        "en": ["About Realtek", "Products", "News", "Investor Relations",
            "Corporate Sustainability Development", "Downloads", "Contact Us", "Careers"],
        "cn": ["", "", "&", "", "",
            "", ""],
        "jp": ["", "", "&",
            "", "", "", ""]
    }

    if (currentLang == "")
        menu_od = footer_menu_order["tw"];
    else if (currentLang == "")
        menu_od = footer_menu_order["cn"];
    else if (currentLang == "")
        menu_od = footer_menu_order["jp"];
    else
        menu_od = footer_menu_order["en"];

    var columnEleCount = Math.floor(menu_od.length / 3);   //ColumnN3Column
    if (menu_od % 3 != 0)
        columnEleCount += 1;

    /////////Create Footer Menu
    var str = "";
    $.each(menu_od, function (i, parent_name) {
        var items = menu_list.find(function (x) {
            return x.Name == parent_name;
        });

        if (items == null)
            return; //continue;

        if (i % columnEleCount == 0) {
            str += ' <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">';
        }

        var footerMenuHtml = $("#footerMenuTemplate").html();
        footerMenuHtml = footerMenuHtml.replace("{footerMenuName}", parent_name);
        var foorerMenuInner = "";
        $.each(items.SubItems, function (i, submenu) {
            var linkTemp = $("#footerMenuLinkTemplate").html();
            linkTemp = linkTemp.replace("{footerlink}", submenu.Url);
            linkTemp = linkTemp.replace("{footerlinktext}", submenu.Name);
            foorerMenuInner += linkTemp;
        });
        footerMenuHtml = footerMenuHtml.replace("{footerMenuInnerModule}", foorerMenuInner);
        str += footerMenuHtml;

        if (i % columnEleCount == 2 || i == menu_od.length - 1) {
            str += '</div>';
        }
    });

    $("#footerMenu").html(str);

</script>
