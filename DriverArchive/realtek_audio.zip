<script>
    var cate_id = getParameterByName("cate_id", window.location.href);
    var menu_id = getParameterByName("menu_id", window.location.href);
    var sel = document.getElementById("select-cate-box");
    var productlist = null;

    if (cate_id != 194) $("#for-network-interface").remove();

    $.ajax({
        type: "GET",
        cache: false,
        url: '/Download/ListAllDownloadItemByCategory?cate_id=' + cate_id,
        contentType: "application/json;",
        dataType: "json",
        success: function (result) {
            if (result.Pass && result.Data?.Id) {
                showDownloadProductList(result.Data, cate_id);
            } else {
                showErrorMessage();
            }
        },
        error: function (result) {
            showErrorMessage();
        },
    });

    $.ajax({
        type: "GET",
        cache: false,
        url: '/Product/GetAllProductTitle',
        contentType: "application/json;",
        dataType: "json",
        async: false,
        success: function (result) {
            if (result.Pass && result.Data) {
                productlist = result.Data;
            } 
        },
        error: function (result) {
            showErrorMessage();
        },
    });

    function showDownloadProductList(data, cate_id) {
        var optText = $("#select-cate-box option:first").html();
        $("#select-cate-box").empty(); 
        $("#mainTitle").html(data.Lang);
    
        productlist[cate_id] = data.Lang;

        // all
        $("#select-cate-box").html("<option value='" + cate_id + "'>" + optText + "</option>");
        
        if (data.DownloadCategoryItems && data.DropdownMenu){
            var checkmenu_id = parseInt(menu_id);
            var nowCate = data.DropdownMenu[cate_id];
            if (nowCate) {
                for (var s = 0; s < nowCate.length; s++) {
                    addCateOption(sel, nowCate[s]);
                }
            } else {
                $("#select-cate-box").empty(); 
                addCateOption(sel, cate_id);
            }

            if (data.DownloadCategoryItems.length == 0) {
                $("#mainTitle").html(productlist[cate_id]);
            }

            for (var i = 0; i < data.DownloadCategoryItems.length; i++) {
                var item = data.DownloadCategoryItems[i];
                if (item == null) return;               

                var DownloadProductTemplate = $("#newItemTemplate").html();
                DownloadProductTemplate = DownloadProductTemplate.replace("{DownloadProductTitle}", item.Title);
                DownloadProductTemplate = DownloadProductTemplate.replace("{DownloadTypeTitle}", item.DownloadType);
                if (checkmenu_id == menu_id) {
                    DownloadProductTemplate = DownloadProductTemplate.replace("{DownloadProductListUrl}", location.origin + item.DownloadCateLink + "&menu_id=" + menu_id);
                } else {
                    DownloadProductTemplate = DownloadProductTemplate.replace("{DownloadProductListUrl}", location.origin + item.DownloadCateLink);
                }
                $("#downloadItems").append(DownloadProductTemplate);
            }
        }
    }

    function addCateOption(sel, val) {
        var opt = new Option(productlist[val], val);
        sel.options.add(opt);
    }

    $("#select-cate-box").on("change", function(){
        var getcate = $("#select-cate-box").val();
        var checkmenu_id = parseInt(menu_id);
        if (getcate != cate_id) {
            if (checkmenu_id == menu_id) {
                location.href = location.origin + "/Download/Index?cate_id=" + getcate + "&menu_id=" + menu_id;
            } else {
                location.href = location.origin + "/Download/Index?cate_id=" + getcate;
            }
        }
    });

    $("#for-network-interface.pos-cate").each(function(i, v){
        $(this).on("click", function(){
            location.href = $(this).children(a).attr('href');
        });
    });	

    function showErrorMessage() {
        var info = JSON.parse("{\u0022content\u0022:\u0022No download items.\u0022,\u0022title\u0022:\u0022No download items\u0022}");
        document.getElementById("mainTitle").innerHTML = info.title;
        document.getElementById("downloadItems").innerHTML = "<div class='col-md-12'>" + info.content + "</div>";
        $("#select-form").hide();
    }
</script>
