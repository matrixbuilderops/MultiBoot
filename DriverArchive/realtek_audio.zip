    var layouyMenuCountId = 1;
    function createNavMenu() {
        var menu = $("#navMenu"); //MenuHtml
        var content = ""; // Menu 

        if (menu_list != null) {

            menu_list.forEach(function (item) {
                var str = ""; // Node 

                //
                if (item.Type == "Node") {

                    layouyMenuCountId += 1;

                    //
                    if (item.SubItems && item.SubItems.length > 0) {
                        //
                        str += "<li class=''>" +
                            "<a class='menu-root dropdown-toggle collapsed' href='#menuId" + layouyMenuCountId + "' data-toggle='collapse' aria-expanded='false'>" +
                            item.Name + '<i class="fa-solid fa-chevron-right  float-right"></i>' +
                            "</a>";
                        // 
                        str += generateSubMenu(item.SubItems, 1);


                    } else {
                        //
                        str += "<li class=''>" +
                            "<a class='menu-root' href='#menuId" + layouyMenuCountId + "' >" +
                            item.Name + "</a>";
                    }
                    str += "</li>";
                }
                else {
                    str = "<li>" +
                        "<a href='" + item.Url + "' title = '" + item.Name + "'>" + item.Name + " </a>" +
                        "</li>"
                }
                content += str;
            });
            menu.prepend(content); //prepend
        }
    }

    function generateSubMenu(subItems, level) {
        var substr = "";
        
        //
        substr += '<ul class="list-unstyled collapse level' + level + '" id="menuId' + layouyMenuCountId + '" style="">';

        $.each(subItems, function (i, item_menu) {
            layouyMenuCountId += 1;
            if (item_menu.SubItems != null) {
                substr += "<li class=''>" + "<a class='menu-root dropdown-toggle collapsed' href='#menuId" +
                    layouyMenuCountId + "' data-toggle='collapse' aria-expanded='false'>" + item_menu.Name + '<i class="fa-solid fa-chevron-right float-right"></i>' +
                    "</a>";
                substr += generateSubMenu(item_menu.SubItems, level + 1);

            } else {

                substr += "<li>" +
                    "<a href='" + item_menu.Url + "' title = '" + item_menu.Name + "'>" + item_menu.Name + "</a>" +
                    "<hr style='margin:0'>" +
                    "</li>";
            }

        });
        substr += "</ul>";
        return substr;
    }




